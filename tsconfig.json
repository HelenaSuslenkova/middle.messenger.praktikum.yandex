{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "target": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "module": "esnext",
    "typeRoots": ["node_modules/@types", "typings/app.d.ts"],
    // "strict": true,
    "baseUrl": "./src",
  },
  "exclude": ["node_modules/**/*"]
}
